<?xml version="1.0" encoding="UTF-8" ?>
<reactiveui:ReactiveContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    x:TypeArguments="vm:CommandBindViewModel"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:vm="clr-namespace:XamarinFormsReactiveUIDemo.ViewModels"
    xmlns:reactiveui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
    x:Class="XamarinFormsReactiveUIDemo.Pages.CommandBindPage">
    <ContentPage.Content>
        <StackLayout>
            <StackLayout Padding="8" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand">
                <Slider Minimum="0" Maximum="100" Value="50" x:Name="slider" />
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</reactiveui:ReactiveContentPage>
