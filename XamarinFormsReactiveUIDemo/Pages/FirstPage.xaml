<?xml version="1.0" encoding="UTF-8" ?>
<reactiveui:ReactiveContentPage
    xmlns="http://xamarin.com/schemas/2014/forms"
    x:TypeArguments="vm:FirstViewModel"
    xmlns:vm="clr-namespace:XamarinFormsReactiveUIDemo.ViewModels"
    xmlns:reactiveui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="XamarinFormsReactiveUIDemo.Pages.FirstPage">
    <ContentPage.Content>
        <StackLayout Padding="8">
            <Entry Text="{Binding Message}" Margin="2" Placeholder="Enter message here..." />
            <Button Text="Show New Page" Command="{Binding NavigateCommand}" Margin="2" />
        </StackLayout>
    </ContentPage.Content>
</reactiveui:ReactiveContentPage>
